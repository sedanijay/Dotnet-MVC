@model IEnumerable<InterviewTask.Models.productModel>

<br /><br />
<p>
    @Html.ActionLink("Create New Product", "Create")
</p>
<br />
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.cat_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cat_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.prod_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.prod_name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.cat_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cat_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.prod_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.prod_name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new {  id=item.prod_id }) |
            @*@Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |*@
            @Html.ActionLink("Delete", "Delete", new { id = item.prod_id })
        </td>
    </tr>
}

</table>
